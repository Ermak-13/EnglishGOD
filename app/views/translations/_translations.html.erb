<div id="last-words">
  <table class="table table-hover">
    <tr>
      <th>#</th>
      <th>Egnlish</th>
      <th>Russian</th>
      <th>Context</th>
    </tr>
    <% dictionary.each_with_index do |dictionary_el, i| %>
      <tr>
        <td><%= dictionary.count - i %></td>
        <td><%= dictionary_el.word %></td>
        <td><%= dictionary_el.translation.value %></td>
        <!-- TODO -->
        <td><span class="temp">bla-bla-bla</span></td>
      </tr>
    <% end %>
  </table>
</div>